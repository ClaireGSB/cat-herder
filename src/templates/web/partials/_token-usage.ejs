<!-- Token Usage & Stats -->
<% if (tokenUsage && Object.keys(tokenUsage).length > 0) { %>
    <div class="row mb-4">
        <div class="col-12">
            <div class="card">
                <div class="card-header bg-transparent">
                    <h5 class="card-title mb-0">
                        <i class="bi bi-graph-up me-2"></i>
                        Token Usage
                    </h5>
                </div>
                <div class="card-body">
                    <div class="row">
                        <% Object.entries(tokenUsage).forEach(([model, usage]) => { %>
                            <div class="col-lg-6 mb-3">
                                <div class="task-stats h-100">
                                    <h6 class="mb-3">
                                        <i class="bi bi-cpu me-1"></i>
                                        <%= helpers.formatModelName(model) %>
                                    </h6>
                                    
                                    <div class="row text-center">
                                        <div class="col-md-3 col-6 mb-2">
                                            <h5 class="text-primary mb-0"><%= usage.inputTokens?.toLocaleString() || '0' %></h5>
                                            <small class="text-muted">Input</small>
                                        </div>
                                        <div class="col-md-3 col-6 mb-2">
                                            <h5 class="text-success mb-0"><%= usage.outputTokens?.toLocaleString() || '0' %></h5>
                                            <small class="text-muted">Output</small>
                                        </div>
                                        <div class="col-md-3 col-6 mb-2">
                                            <h5 class="text-info mb-0"><%= usage.cacheCreationInputTokens?.toLocaleString() || '0' %></h5>
                                            <small class="text-muted">Cache Write</small>
                                        </div>
                                        <div class="col-md-3 col-6 mb-2">
                                            <h5 class="text-warning mb-0"><%= usage.cacheReadInputTokens?.toLocaleString() || '0' %></h5>
                                            <small class="text-muted">Cache Read</small>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <% }); %>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% } %>