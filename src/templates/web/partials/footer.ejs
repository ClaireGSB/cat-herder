    </div> <!-- container -->
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    
    <!-- Core dashboard functionality -->
    <script src="/js/dashboard.js"></script>
    
    <!-- Live activity specific functions -->
    <script src="/js/live-activity.js"></script>
    
    <!-- Page-specific scripts -->
    <% if (typeof page !== 'undefined') { %>
        <% if (page === 'dashboard') { %>
            <script src="/js/dashboard-page.js"></script>
        <% } else if (page === 'task-detail') { %>
            <script src="/js/task-detail-page.js"></script>
        <% } else if (page === 'sequence-detail') { %>
            <script src="/js/sequence-detail-page.js"></script>
        <% } else if (page === 'sequences-dashboard') { %>
            <script src="/js/sequences-dashboard-page.js"></script>
        <% } else if (page === 'live-activity') { %>
            <!-- Set up live activity data before loading page script -->
            <script>
                window.liveActivityData = {
                    runningTask: <%- typeof runningTask !== 'undefined' && runningTask ? JSON.stringify(runningTask) : 'null' %>,
                    parentSequence: <%- typeof parentSequence !== 'undefined' && parentSequence ? JSON.stringify(parentSequence) : 'null' %>
                };
            </script>
            <script src="/js/live-activity-page.js"></script>
        <% } %>
    <% } %>
</body>
</html>